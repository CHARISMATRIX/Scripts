--SMAGARD HUB CREATED BY SMAGARD

--Variables
local lib = loadstring(game:HttpGet"https://raw.githubusercontent.com/dawid-scripts/UI-Libs/main/Vape.txt")()

local plr = game:GetService("Players").LocalPlayer
local char = plr.Character or plr.CharacterAdded:Wait()
local mouse = plr:GetMouse()

local Tabs = {}

--Refresh Character
plr.CharacterAdded:Connect(function(newchar)
	char = newchar
end)

--UI

local win = lib:Window("SMAGARD-Hub", Color3.fromRGB(44, 120, 224), Enum.KeyCode.RightControl)

--MAIN
Tabs.Main = win:Tab("Main")

Tabs.Main:Label("Credits: SMAGARD")
Tabs.Main:Button("Destroy UI", function()
	game:GetService("CoreGui"):FindFirstChild("ui"):Destroy()
end)

--UNIVERSAL
Tabs.Universal = win:Tab("Universal")

--ClickTP
Tabs.Universal:Button("Click TP", function()
	local tool = Instance.new("Tool")
	tool.RequiresHandle = false
	tool.Name = "Click Teleport"
	tool.Activated:connect(function()
		local pos = mouse.Hit+Vector3.new(0,2.5,0)
		pos = CFrame.new(pos.X,pos.Y,pos.Z)
		char.HumanoidRootPart.CFrame = pos
	end)
	tool.Parent = plr.Backpack
end)
--AutoClicker
Tabs.Universal:Button("Auto Clicker", function()
	loadstring(game:HttpGetAsync("https://raw.githubusercontent.com/JustEzpi/ROBLOX-Scripts/main/ROBLOX_AutoClicker"))()
end)

--Player
Tabs.Player = win:Tab("Player")

--Respawn
Tabs.Player:Button("Respawn", function()
	if char:FindFirstChildOfClass("Humanoid") then char:FindFirstChildOfClass("Humanoid"):ChangeState(15) end
	char:ClearAllChildren()
	local newChar = Instance.new("Model")
	newChar.Parent = workspace
	plr.Character = newChar
	wait()
	plr.Character = char
	newChar:Destroy()
end)
--UnlockCameraZoom
Tabs.Player:Button("MaxCameraZoom", function()
	plr.CameraMaxZoomDistance = math.huge
end)
--WalkSpeed
Tabs.Player:Slider("Speed", 16,1000,16, function(s)
	char.Humanoid.WalkSpeed = s
end)
--JumpPower
Tabs.Player:Slider("JumpPower", 50,500,50, function(s)
	char.Humanoid.JumpPower = s
end)
--R6
Tabs.Player:Button("R6", function()
	loadstring(game:HttpGet("https://pastebin.com/raw/E9LkqrVb"))()
end)

