Tabs.ScriptHubs:Button("Eclipse Hub", function()
	local a,b,c,d,e=loadstring,request or http_request or (http and http.request) or (syn and syn.request),assert,tostring,"https\58//api.eclipsehub.xyz/auth";c(a and b,"Executor not Supported")a(b({Url=e.."\?\107e\121\61"..d("nil"),Headers={["User-Agent"]="Eclipse"}}).Body)()
end)

--BABFT Autobuild
Tabs.ScriptHubs:Button("BABFT Autobuild", function()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/max2007killer/auto-build-not-limit/main/autobuild.txt"))()
end)

--Current Game
if game.PlaceId == 537413528 then --Build a Boat
	Tabs.BABFT = win:Tab("Build A Boat")

	--AutoFarm
	function Farm()
		game.workspace.Gravity = 0
		char:WaitForChild("HumanoidRootPart").CFrame = CFrame.new(-51.1823959, 80.6168747, -536.437805)
		local tweenService, tweenInfo = game:GetService("TweenService"), TweenInfo.new(30, Enum.EasingStyle.Linear)
		local tween =
			tweenService:Create(
				char:WaitForChild("HumanoidRootPart"),
				tweenInfo,
				{CFrame = CFrame.new(-60.5737877, 53.9498825, 8666.35059)}
			)
		tween:Play()
		wait(30)
		tweenService, tweenInfo = game:GetService("TweenService"), TweenInfo.new(0, Enum.EasingStyle.Linear)
		tween =
			tweenService:Create(
				char:WaitForChild("HumanoidRootPart"),
				tweenInfo,
				{CFrame = CFrame.new(-55.5486526, -360.063782, 9489.0498)}
			)
		tween:Play()
		game.workspace.Gravity = 196.2
	end

	Tabs.BABFT:Button("Farm Once", function()
		Farm()
	end)

	--Teleports
	Tabs.BABFT:Button("White Team", function()
		char.HumanoidRootPart.CFrame = game:GetService("Workspace").WhiteZone.CFrame * CFrame.new(0,5,0)
	end)

	Tabs.BABFT:Button("Black Team", function()
		char.HumanoidRootPart.CFrame = game:GetService("Workspace").BlackZone.CFrame * CFrame.new(0,5,0)
	end)

	Tabs.BABFT:Button("Red Team", function()
		char.HumanoidRootPart.CFrame = game:GetService("Workspace")["Really redZone"].CFrame * CFrame.new(0,5,0)
	end)

	Tabs.BABFT:Button("Blue Team", function()
		char.HumanoidRootPart.CFrame = game:GetService("Workspace")["Really blueZone"].CFrame * CFrame.new(0,5,0)
	end)

	Tabs.BABFT:Button("Green Team", function()
		char.HumanoidRootPart.CFrame = game:GetService("Workspace").CamoZone.CFrame * CFrame.new(0,5,0)
	end)

	Tabs.BABFT:Button("Yellow Team", function()
		char.HumanoidRootPart.CFrame = game:GetService("Workspace")["New YellerZone"].CFrame * CFrame.new(0,5,0)
	end)

	Tabs.BABFT:Button("Magenta Team", function()
		char.HumanoidRootPart.CFrame = game:GetService("Workspace").MagentaZone.CFrame * CFrame.new(0,5,0)
	end)

	--Water Godmode
	Tabs.BABFT:Button("Water GodMode", function()
		char:WaitForChild("WaterDetector"):remove()
	end)

	--Bypass Isolation
	Tabs.BABFT:Button("Bypass Isolation", function()
		if game:GetService("Workspace").WhiteZone:FindFirstChild("Lock") then
			game:GetService("Workspace").WhiteZone:FindFirstChild("Lock").Part:Destroy()
		end
		if game:GetService("Workspace").BlackZone:FindFirstChild("Lock") then
			game:GetService("Workspace").BlackZone:FindFirstChild("Lock").Part:Destroy()
		end
		if game:GetService("Workspace")["Really redZone"]:FindFirstChild("Lock") then
			game:GetService("Workspace")["Really redZone"]:FindFirstChild("Lock").Part:Destroy()
		end
		if game:GetService("Workspace")["Really blueZone"]:FindFirstChild("Lock") then
			game:GetService("Workspace")["Really blueZone"]:FindFirstChild("Lock").Part:Destroy()
		end
		if game:GetService("Workspace").CamoZone:FindFirstChild("Lock") then
			game:GetService("Workspace").CamoZone:FindFirstChild("Lock").Part:Destroy()
		end
		if game:GetService("Workspace")["New YellerZone"]:FindFirstChild("Lock") then
			game:GetService("Workspace")["New YellerZone"]:FindFirstChild("Lock").Part:Destroy()
		end
		if game:GetService("Workspace").MagentaZone:FindFirstChild("Lock") then
			game:GetService("Workspace").MagentaZone:FindFirstChild("Lock").Part:Destroy()
		end
	end)

elseif game.PlaceId == 3956818381 then --Ninja Legends
	Tabs.NinjaLegends = win:Tab("Ninja Legends")

	--AutoSwing
	Tabs.NinjaLegends:Toggle("AutoSwing", function(state)
		getgenv().autoswing = state
		while wait(.1) and getgenv().autoswing do
			if not getgenv().autoswing then return end
			for _,v in pairs(plr.Backpack:GetChildren()) do
				if v:FindFirstChild("ninjitsuGain") then
					char.Humanoid:EquipTool(v)
					break
				end
			end
			plr.ninjaEvent:FireServer("swingKatana")
		end
	end)  
	--AutoSell
	Tabs.NinjaLegends:Toggle("AutoSell",false, function(state)
		getgenv().autosell = state
		while wait(.1) and getgenv().autosell do
			if getgenv().autosell == false then return end
			game:GetService("Workspace").sellAreaCircles["sellAreaCircle16"].circleInner.CFrame = char.HumanoidRootPart.CFrame
			wait(0.1)
			game:GetService("Workspace").sellAreaCircles["sellAreaCircle16"].circleInner.CFrame = CFrame.new(0,0,0)
		end
	end)
	--AutoBuySwords
	Tabs.NinjaLegends:Toggle("Auto buy swords",false, function(state)
		getgenv().autobuyswords = state
		while wait(.5) and getgenv().autobuyswords do
			if not getgenv().autobuyswords then return end
			local args = {
				[1] = "buyAllSwords",
				[2] = "Inner Peace Island"
			} 
			plr.ninjaEvent:FireServer(unpack(args))
		end
	end)
	--AutoBuyBelts
	Tabs.NinjaLegends:Toggle("Auto buy belts",false, function(state)
		getgenv().autobuybelts = state
		while wait(.5) and getgenv().autobuybelts do
			if not getgenv().autobuybelts then return end
			local args = {
				[1] = "buyAllBelts",
				[2] = "Inner Peace Island"
			} 
			plr.ninjaEvent:FireServer(unpack(args))
		end
	end)
	--AutoBuySkills
	Tabs.NinjaLegends:Toggle("Auto buy Skills",false, function(state)
		getgenv().autobuyskills = state
		while wait(.5) and getgenv().autobuyskills do
			if not getgenv().autobuyskills then return end
			local args = {
				[1] = "buyAllSkills",
				[2] = "Inner Peace Island"
			} 
			plr.ninjaEvent:FireServer(unpack(args))
		end
	end)
	--AutoBuyShurikens
	Tabs.NinjaLegends:Toggle("Auto buy Shurikens",false, function(state)
		getgenv().autobuyshurikens = state
		while wait(.5) and getgenv().autobuyshurikens do
			if not getgenv().autobuyshurikens then return end
			local args = {
				[1] = "buyAllShurikens",
				[2] = "Inner Peace Island"
			} 
			plr.ninjaEvent:FireServer(unpack(args))
		end
	end)
	--AutoHoops
	Tabs.NinjaLegends:Toggle("Auto Hoops",false, function(state)
		getgenv().autohoops = state
		while wait() and getgenv().autohoops do
			if not getgenv().autohoops then return end
			for _,v in pairs(game:GetService("Workspace").Hoops:GetChildren()) do
				local args = {
					[1] = "useHoop",
					[2] = v
				}

				game:GetService("ReplicatedStorage").rEvents.hoopEvent:FireServer(unpack(args))
			end
		end
	end)
	--AutoCoins
	Tabs.NinjaLegends:Toggle("Auto Coins",false, function(state)
		getgenv().autocoins = state
		while wait(.1) and getgenv().autocoins do
			if not getgenv().autocoins then return end
			for _, v in pairs(game:GetService("Workspace").spawnedCoins.Valley:GetChildren()) do
				char.HumanoidRootPart.CFrame = v.CFrame
			end
		end
	end)
	--UnlockAllIslands
	Tabs.NinjaLegends:Button("Unlock all islands", function()
		local oldcframe = char.HumanoidRootPart.CFrame
		for _,v in pairs(game:GetService("Workspace").islandUnlockParts:GetChildren()) do
			char.HumanoidRootPart.CFrame = v.CFrame
			wait(0.1)
		end
		wait(0.1)
		char.HumanoidRootPart.CFrame = oldcframe
	end)

elseif game.PlaceId == 155615604 then --Prison Life
	Tabs.PrisonLife = win:Tab("Prison Life")

	--Give Gun
	Tabs.PrisonLife:Dropdown("Give Gun", {"M9", "Remington 870", "AK-47"}, function(currentOption)
		local args = {
			[1] = workspace.Prison_ITEMS.giver:FindFirstChild(currentOption).ITEMPICKUP
		}

		workspace.Remote.ItemHandler:InvokeServer(unpack(args))
	end)
	--Gun Mod
	Tabs.PrisonLife:Dropdown("Gun Mod", {"M9", "Remington 870", "AK-47"}, function(currentOption)
		local module = nil
		if plr.Backpack:FindFirstChild(currentOption) then
			module = require(plr.Backpack[currentOption].GunStates)
		elseif char:FindFirstChild(currentOption) then
			module = require(char[currentOption].GunStates)
		end
		if module ~= nil then
			module["MaxAmmo"] = math.huge
			module["CurrentAmmo"] = math.huge
			module["StoredAmmo"] = math.huge
			module["FireRate"] = 0.000001
			module["Spread"] = 0
			module["Range"] = math.huge
			module["Bullets"] = 10
			module["ReloadTime"] = 0.000001
			module["AutoFire"] = true
		end
	end)

elseif game.PlaceId == 12524383962 then --Every Second +1 Ki in DBZ
	Tabs.EverySecondKi = win:Tab("Every Second +1 Ki")

	--Autokill
	Tabs.EverySecondKi:Toggle("Autokill", false, function(s)
		getgenv().autokill = s

		while autokill and task.wait(.05) do
			if not autokill then return end

			for _,model in pairs(workspace:WaitForChild("World"):GetChildren()) do
				if not autokill then return end

				if not model:IsA("Model") or not tonumber(model.Name) then continue end

				for _, part in pairs(model:GetChildren()) do
					if not autokill then return end

					if not part:IsA("Part") then continue end

					local args = {
						[1] = part,
						[2] = true,
						[3] = part.CFrame
					}
					game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("Player"):WaitForChild("DmgHandle"):FireServer(unpack(args))
				end
			end
		end
	end)

	--Autocollect
	Tabs.EverySecondKi:Toggle("Autocollect", false, function(s)
		getgenv().autocollect = s

		while autocollect and task.wait(.05) do
			if not autocollect then return end
			for _,object in pairs(workspace:GetChildren()) do
				if not autocollect then return end

				if not object:IsA("MeshPart") or not string.find(object.Name, "draw") then continue end

				firetouchinterest(plr.Character.Head, object, 0)

			end
		end
	end)

	--Auto Rebirth
	Tabs.EverySecondKi:Toggle("Auto Rebirth", false, function(s)
		getgenv().autorebirth = s
		while autorebirth and task.wait(.5) do
			if not autorebirth then return end
			game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("Player"):WaitForChild("RebirthEvent"):FireServer()
		end
	end)
	
	--Hide Collections
	Tabs.EverySecondKi:Button("Hide Collections", function()
		plr.PlayerGui.EffectRefersh:Destroy()
	end)

elseif game.PlaceId == 12888107609 then --Wall Punch Simulator
	
	Tabs.WallPunchSim = win:Tab("Wall Punch Simulator")

	Tabs.WallPunchSim:Textbox("Amount Of Coins", true, function(v)
		local args = {
    		[1] = game:GetService("Players").LocalPlayer,
    		[2] = tonumber(v)
		}

		game:GetService("ReplicatedStorage"):WaitForChild("NL"):FireServer(unpack(args))
	end)

	Tabs.WallPunchSim:Textbox("Amount Of Power", true, function(v)
		local args = {
    		[1] = game:GetService("Players").LocalPlayer,
    		[2] = tonumber(v)
		}

		game:GetService("ReplicatedStorage"):WaitForChild("GP"):FireServer(unpack(args))
	end)

	Tabs.WallPunchSim:Textbox("Extra Multiplier", true, function(v)
		game:GetService("Players").LocalPlayer.ExtraMultiplier.Value = tonumber(v)		
	end)

	Tabs.WallPunchSim:Toggle("Autoclick", false, function(v)
		game:GetService("Players").LocalPlayer.AutoClick.Value = v
	end)

elseif game.PlaceId == 662417684 then
	
	Tabs.LBBattlegrounds = win:Tab("Lucky Block Battlegrounds")

	Tabs.LBBattlegrounds:Slider("Amount Of Blocks", 1,30,1, function(v)
		getgenv().blockamount = tonumber(v)
	end)

	Tabs.LBBattlegrounds:Button("Spawn Default", function()
		for i=1,getgenv().blockamount,1 do
			game:GetService("ReplicatedStorage"):WaitForChild("SpawnLuckyBlock"):FireServer()
		end
	end)

	Tabs.LBBattlegrounds:Button("Spawn Super", function()
		for i=1,getgenv().blockamount,1 do
			game:GetService("ReplicatedStorage"):WaitForChild("SpawnSuperBlock"):FireServer()
		end
	end)

	Tabs.LBBattlegrounds:Button("Spawn Diamond", function()
		for i=1,getgenv().blockamount,1 do
			game:GetService("ReplicatedStorage"):WaitForChild("SpawnDiamondBlock"):FireServer()
		end
	end)

	Tabs.LBBattlegrounds:Button("Spawn Rainbow", function()
		for i=1,getgenv().blockamount,1 do
			game:GetService("ReplicatedStorage"):WaitForChild("SpawnRainbowBlock"):FireServer()
		end
	end)

	Tabs.LBBattlegrounds:Button("Spawn Galaxy", function()
		for i=1,getgenv().blockamount,1 do
			game:GetService("ReplicatedStorage"):WaitForChild("SpawnGalaxyBlock"):FireServer()
		end
	end)

